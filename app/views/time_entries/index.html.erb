<p id="notice"><%= notice %></p>

<h1>Time Entries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timeentries.each do |timeentry| %>
      <tr>
        <td><%= project_id.name %></td>
        <td><%= developer_id.name %></td>
        <td><%= duration %></td>
        <td><%= date %></td>
        <td><%= link_to 'Show', timeentry %></td>
        <td><%= link_to 'Edit', edit_time_entry_path(timeentry) %></td>
        <td><%= link_to 'Destroy', timeentry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back to Time Entries', time_entries_path %> <br>
<%= link_to 'Create New Time Entry', new_time_entry_path %>
